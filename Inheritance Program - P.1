public class Ship implements Comparable<Ship> {
	
//-------------------------INSTANCE VARIABLES-----------------------//

	private String shipName;
	private int yearBuilt;
	
//---------------------------CONSTRUCTOR---------------------------//

	public Ship(String shipName,int yearBuilt) {
		this.shipName = shipName;
		this.yearBuilt = yearBuilt;
	}

//---------------------------METHODS-------------------------------//
	
	//mutator
	public void setName(String shipName) {
		this.shipName = shipName;
	}
	
	public void setYear(int yearBuilt) {
		this.yearBuilt = yearBuilt;
	}
		
	//getter
	public String getName() {
		return shipName;
	}
	
	//getter
	public int getYear() {
		return yearBuilt;
	}
	
	//toString method to display ship name and year built
	public String toString() {
		return shipName + " built in " + yearBuilt; 
	}
	
	//compareTo method using implemented interface 
	public int compareTo(Ship s) {
		int value = shipName.compareTo(s.getName());
		return value;
	}
	
//------------------------------MAIN---------------------------------//

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
